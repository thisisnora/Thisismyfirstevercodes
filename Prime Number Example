#include <iostream>

using namespace std;

int main()
{
    int is_prime= 1;
    int count=0;
    cout << "\n Prime numbers between 50 and 100 are: ";

    for(int n=50;n<=100;n++){
        is_prime= 1;
        for(int i=2;i<n;i++){
            if(n%i==0){
                is_prime= 0;
        
            }
        }
        if(is_prime){
            cout<<n<<" "<<endl;
            count=count+1;
        }
       
    }
    cout<<"Number of Prime Numbers Between 50 and 100 is:"<<count;
     return 0;
}
